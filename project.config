[project]
	description = Repository for tests
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/diakovliev
	owner = group user/yuriamw
[access "refs/for/refs/*"]
	read = group user/diakovliev
	read = group user/yuriamw
	push = group user/diakovliev
	push = group user/yuriamw
	label-Code-Review = -2..+2 group user/diakovliev
	label-Code-Review = -2..+2 group user/yuriamw
	label-Verified = -1..+1 group user/diakovliev
	label-Verified = -1..+1 group user/yuriamw
	rebase = group user/diakovliev
	rebase = group user/yuriamw
	removeReviewer = group user/diakovliev
	removeReviewer = group user/yuriamw
	submit = group user/diakovliev
	submit = group user/yuriamw
[access "refs/heads/*"]
	read = group user/diakovliev
	read = group user/yuriamw
	create = group user/diakovliev
	create = group user/yuriamw
	push = +force group user/diakovliev
	push = group user/yuriamw
	pushMerge = group user/diakovliev
	pushMerge = group user/yuriamw
[access "refs/tags/*"]
	push = +force group user/diakovliev
	push = group user/yuriamw
	pushTag = group user/diakovliev
	pushTag = group user/yuriamw
	pushSignedTag = group user/diakovliev
	pushSignedTag = group user/yuriamw
[submit]
	action = cherry pick

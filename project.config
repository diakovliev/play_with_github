[project]
	description = Repository for tests
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/diakovliev
	forgeCommitter = group user/diakovliev
	forgeCommitter = group user/yuriamw (yuriamw)
	push = group user/diakovliev
	read = group user/diakovliev
	read = group user/yuriamw (yuriamw)
	create = group user/diakovliev
[access "refs/heads/*"]
	forgeAuthor = group user/diakovliev
	forgeAuthor = group user/yuriamw (yuriamw)
	label-Code-Review = -2..+2 group user/diakovliev
	label-Code-Review = -2..+2 group user/yuriamw (yuriamw)
	label-Verified = -1..+1 group user/diakovliev
	label-Verified = -1..+1 group user/yuriamw (yuriamw)
	submit = group user/diakovliev
[access "refs/tags/*"]
	pushTag = group user/diakovliev
	pushTag = group user/yuriamw (yuriamw)
[submit]
	action = rebase if necessary
[access "refs/for/refs/*"]
	forgeAuthor = group user/diakovliev
	push = group user/diakovliev
	push = group user/yuriamw (yuriamw)
	pushMerge = group user/diakovliev
	pushMerge = group user/yuriamw (yuriamw)
